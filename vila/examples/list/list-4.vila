fun test(li)
{
    print(li[0]);
    return li[0];
}

mut l = test([1, 2, 3]);
print(l);

class Test
{
    init(list)
    {
        this.list = list;
    }

    print_list()
    {
        print(this.list[0]);
    }

    return_list()
    {
        return this.list[0];
    }
}

mut t = Test([1, 2, 3]);

t.print_list();

mut li = t.return_list();
print(li);

