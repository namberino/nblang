lớp Node {
    khởi_tạo(data) {
        đây.data = data;
        đây.next = trống;
    }
}

lớp LinkedList {
    khởi_tạo() {
        đây.head = trống;
    }

    print_list() {
        biến value = đây.head;

        khi (value != trống) {
            in(value.data);
            value = value.next;
        }
    }
}

biến ll = LinkedList();
ll.head = Node(1);
biến second = Node(2);
biến third = Node(3);

ll.head.next = second;
second.next = third;
ll.print_list();
